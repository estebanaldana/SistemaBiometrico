<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Variables</servlet-name>
        <servlet-class>JAVA.Variables</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ejemplo</servlet-name>
        <servlet-class>JAVA.Ejemplo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Variables</servlet-name>
        <url-pattern>/Datos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ejemplo</servlet-name>
        <url-pattern>/Ejemplo</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
        <welcome-file-list>
            <welcome-file>
                knearestneighbour
            </welcome-file>
        </welcome-file-list>
    </session-config>
</web-app>
